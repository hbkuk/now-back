<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.now.mapper.ManagerMapper">

    <!-- 사용자 확인 -->
    <select id="findById" parameterType="java.lang.String" resultType="Manager">
        SELECT *
        FROM tb_manager
        WHERE id = #{id}
    </select>

</mapper>
